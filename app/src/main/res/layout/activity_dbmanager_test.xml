<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dgsw.hs.kr.flow.Activity.DBManagerTestActivity"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/btn_dbselect"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="51dp"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="44dp"
        android:text="SELECT TOKEN"
        app:layout_constraintBottom_toTopOf="@+id/tv_resultOfSelect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_resultOfSelect"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="32dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/btn_insertTest"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_dbselect" />

    <Button
        android:id="@+id/btn_insertTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="342dp"
        android:text="Insert Test"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tv_resultOfSelect"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultOfSelect" />

    <ListView
        android:id="@+id/lv_resultdb"
        android:layout_width="391dp"
        android:layout_height="230dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="364dp" />

    <Button
        android:id="@+id/btn_selecttest"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="302dp" />
</android.support.constraint.ConstraintLayout>
