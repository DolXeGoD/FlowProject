<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dgsw.hs.kr.flow.MainActivity"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/nextMealBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다음 급식"
        app:layout_constraintBaseline_toBaselineOf="@+id/choiceMealBtn"
        app:layout_constraintEnd_toStartOf="@+id/choiceMealBtn"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/choiceMealBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginTop="204dp"
        android:text="급식 선택하기"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nextMealBtn"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>
